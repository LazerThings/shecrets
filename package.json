{
	"name": "shecrets",
	"version": "0.0.1",
	"description": "Encrypted secrets files safe to commit. AES-256-GCM + Argon2id, CLI + TUI browser, keychain integration.",
	"license": "MIT",
	"author": "LazerThings",
	"repository": {
		"type": "git",
		"url": "https://github.com/LazerThings/shecrets.git"
	},
	"keywords": [
		"secrets",
		"encryption",
		"aes-256-gcm",
		"argon2",
		"cli",
		"tui",
		"keychain",
		"password-manager"
	],
	"bin": {
		"shecrets": "dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc && chmod +x dist/cli.js",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"argon2": "^0.44.0",
		"better-sqlite3": "^12.6.2",
		"clipboardy": "^5.1.0",
		"ink": "^4.1.0",
		"meow": "^11.0.0",
		"react": "^18.2.0",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/react": "^18.0.32",
		"@types/uuid": "^11.0.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"pnpm": {
		"onlyBuiltDependencies": [
			"argon2",
			"better-sqlite3"
		]
	}
}
